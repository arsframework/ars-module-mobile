<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="
	http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.2.xsd">

	<!-- Mobile module configuration -->
	
	<!-- Apper Repository -->
	<bean class="ars.module.mobile.repository.StandardApperRepository"/>
	
	<!-- Push Repository -->
	<bean class="ars.module.mobile.repository.StandardPushRepository"/>
	
	<!-- Mobile auth Service -->
	<bean class="ars.module.mobile.service.MobileAuthService"/>
	
	<!-- Apper Service -->
	<bean class="ars.module.mobile.service.StandardApperService"/>
	
	<!-- Push Service -->
	<bean class="ars.module.mobile.service.StandardPushService"/>
</beans>